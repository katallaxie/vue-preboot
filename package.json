{
  "name": "vue-preboot",
  "version": "1.0.4",
  "description": "A teeny Vue.js template to get your next project booted. It is opinionated and powered by Webpack",
  "keywords": [
    "vue",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Sebastian DÃ¶ll",
    "email": "sebastian@katallaxie.me"
  },
  "bundlesize": [{
      "path": "./public/main.*.js",
      "maxSize": "250 kB"
    },
    {
      "path": "./public/polyfills.*.js",
      "maxSize": "25 kB"
    }
  ],
  "homepage": "https://github.com/katallaxie/vue-preboot",
  "license": "MIT",
  "scripts": {
    "build:docker": "npm run build && docker build --squash -t vue-preboot:latest .",
    "build:server": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build:client": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build": "npm run build:client && npm run build:server",
    "ci": "npm test --runInBand && bundlesize",
    "clean:public": "npm run rimraf -- public",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm run clean:public && start",
    "clean": "npm cache clean --force && npm run rimraf -- doc coverage public node_modules",
    "cross-env": "cross-env",
    "dev": "cross-env NODE_ENV=development",
    "docker": "docker",
    "help": "node node_modules/npm-scripts-help",
    "jest": "jest",
    "rimraf": "rimraf",
    "prebuild": "npm run clean:public",
    "preversion": "npm test",
    "prestart": "npm run clean:public",
    "prod": "cross-env NODE_ENV=production",
    "server": "cross-env NODE_ENV=production ./node_modules/.bin/mango-ssr --serve public --bundle public/vue-ssr-server-bundle.json -m public/vue-ssr-client-manifest.json -t public/index.html -w config/configs",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/mango-ssr --serve public --bundle public/vue-ssr-server-bundle.json -m public/vue-ssr-client-manifest.json -t public/index.html -w config/configs",
    "test": "cross-env NODE_ENV=testing npm run jest",
    "test:bundlesize": "npm run bundlesize",
    "watch:test": "jest --watch",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@axelspringer/mango-ssr": "^0.2.2",
    "@types/node": "^9.3.0",
    "core-js": "^2.5.1",
    "cross-env": "^5.1.1",
    "ts-node": "^5.0.0",
    "tslib": "^1.5.0",
    "typescript": "2.7.2",
    "vue": "^2.5.8",
    "vue-class-component": "^6.1.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.8",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.3",
    "@types/jest": "^22.0.0",
    "@types/uglify-js": "^2.6.29",
    "autoprefixer": "^8.0.0",
    "avoriaz": "^6.0.1",
    "awesome-typescript-loader": "^4.0.0-0",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "brotli-webpack-plugin": "^0.5.0",
    "bundlesize": "^0.16.0",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "csx": "^8.5.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "html-elements-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "http-server": "^0.11.0",
    "jest": "^22.0.0",
    "npm-run-all": "^4.1.2",
    "npm-scripts-help": "^0.8.0",
    "offline-plugin": "^4.8.4",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.0",
    "ts-jest": "^22.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "^3.5.1",
    "typestyle": "^1.5.1",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "vue-loader": "^14.0.0",
    "vue-jest": "^2.1.0",
    "vue-property-decorator": "^6.0.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "4.1.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-dev-server": "^3.0.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.es2015.json",
        "useBabelrc": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katallaxie/vue-preboot.git"
  },
  "bugs": {
    "url": "https://github.com/katallaxie/vue-preboot/issues"
  },
  "engines": {
    "node": ">= 6.11.5",
    "npm": ">= 3"
  }
}
