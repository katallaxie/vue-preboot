{
  "name": "vue-preboot",
  "version": "1.0.7",
  "description": "A teeny Vue.js template to get your next project booted. It is opinionated and powered by Webpack",
  "keywords": [
    "vue",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Sebastian DÃ¶ll",
    "email": "sebastian@katallaxie.me"
  },
  "browserslist": [
    "last 2 versions",
    ">1%"
  ],
  "bundlesize": [{
      "path": "./public/main.*.js",
      "maxSize": "250 kB"
    },
    {
      "path": "./public/polyfills.*.js",
      "maxSize": "25 kB"
    }
  ],
  "homepage": "https://github.com/katallaxie/vue-preboot",
  "license": "MIT",
  "scripts": {
    "build:docker": "npm run build && docker build --squash -t vue-preboot:latest .",
    "build:server": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build:client": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build": "npm run build:client && npm run build:server",
    "ci": "npm-run-all -s test:ci",
    "e2e": "npm-run-all -p -r server test:e2e",
    "clean:public": "npm run rimraf -- public",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm run clean:public && start",
    "clean": "npm cache clean --force && npm run rimraf -- doc coverage public node_modules",
    "cross-env": "cross-env",
    "dev": "cross-env NODE_ENV=development",
    "docker": "docker",
    "help": "node node_modules/npm-scripts-help",
    "jest": "jest",
    "rimraf": "rimraf",
    "prebuild": "npm run clean:public",
    "preversion": "npm test",
    "prestart": "npm run clean:public",
    "prod": "cross-env NODE_ENV=production",
    "server": "./node_modules/.bin/fluffy --bundle public/vue-ssr-server-bundle.json --manifest public/vue-ssr-client-manifest.json --template public/index.html",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/fluffy --port 3000 --dev --webpack config/configs --bundle public/vue-ssr-server-bundle.json --manifest public/vue-ssr-client-manifest.json --template public/index.html",
    "test": "cross-env NODE_ENV=test npm run jest -- --coverage --config jest.test.json",
    "test:ci": "npm run test --runInBand",
    "test:e2e": "cross-env NODE_ENV=test npm run jest -- --config jest.e2e.json",
    "test:bundlesize": "bundlesize",
    "watch:test": "jest --watch",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@andersnormal/fluffy": "^1.2.2",
    "@types/node": "^10.7.1",
    "@types/puppeteer": "^1.6.0",
    "core-js": "^2.5.1",
    "cross-env": "^5.1.4",
    "es6-promise": "^4.2.4",
    "ts-node": "^7.0.1",
    "tslib": "^1.5.0",
    "typescript": "2.9.2",
    "typestyle": "^2.0.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.1.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.3",
    "@types/jest": "^23.3.1",
    "@types/uglify-js": "^3.0.3",
    "autoprefixer": "^9.0.0",
    "avoriaz": "^6.0.1",
    "awesome-typescript-loader": "^5.0.0-1",
    "babel-preset-vue-app": "^2.0.0",
    "brotli-webpack-plugin": "^1.0.0",
    "bundlesize": "^0.17.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.0.7",
    "http-server": "^0.11.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "npm-run-all": "^4.1.2",
    "npm-scripts-help": "^0.8.0",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "puppeteer": "^1.7.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-loader": "^3.6.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-jest": "^2.2.1",
    "vue-loader": "^15.3.0",
    "vue-server-renderer": "^2.5.17",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-dev-server": "^3.1.5",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katallaxie/vue-preboot.git"
  },
  "bugs": {
    "url": "https://github.com/katallaxie/vue-preboot/issues"
  },
  "engines": {
    "node": ">= 8.11.4",
    "npm": ">= 5"
  }
}
