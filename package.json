{
  "name": "vue-preboot",
  "version": "1.0.0",
  "description": "A teeny Vue.js template to get your next project booted. It is opinionated and powered by Webpack",
  "keywords": [
    "vue",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Sebastian DÃ¶ll",
    "email": "sebastian@katallaxie.me"
  },
  "bundlesize": [
    {
      "path": "./public/client/vendor.*.js",
      "maxSize": "400 kB"
    },
    {
      "path": "./public/client/polyfills.*.js",
      "maxSize": "120 kB"
    }
  ],
  "homepage": "https://github.com/katallaxie/vue-preboot",
  "license": "MIT",
  "scripts": {
    "build:docker": "npm run build:prod && docker build -t vue-preboot:latest .",
    "build:server": "cross-env NODE_ENV=production webpack --progress --profile",
    "build:client": "cross-env NODE_ENV=production webpack --progress --profile --bail --debug",
    "build": "npm run build:client && npm run build:server",
    "ci": "npm test --runInBand && bundlesize",
    "clean:public": "npm run rimraf -- public",
    "clean:client": "npm run rimraf -- public/client",
    "clean:server": "npm run rimraf -- public/*.json",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm run clean:public && start",
    "clean": "npm cache clean --force && npm run rimraf -- doc coverage public node_modules",
    "cross-env": "cross-env",
    "dev": "cross-env NODE_ENV=development",
    "docker": "docker",
    "help": "node node_modules/npm-scripts-help",
    "jest": "jest",
    "rimraf": "rimraf",
    "prebuild:server": "npm run clean:server",
    "prebuild:client": "npm run clean:client",
    "preversion": "npm test",
    "prestart": "npm run clean:public",
    "prod": "cross-env NODE_ENV=production",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server --open --progress --profile --watch",
    "server:prod": "http-server public -c-1 --cors",
    "server:ssr": "ts-node ./server/app",
    "server": "cross-env NODE_ENV=production npm run server:ssr",
    "start": "cross-env NODE_ENV=development npm run server:ssr",
    "test": "cross-env NODE_ENV=testing npm run jest",
    "test:bundlesize": "npm run bundlesize",
    "watch:test": "jest --watch",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "core-js": "^2.5.1",
    "vue": "^2.5.8",
    "vue-class-component": "^6.1.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "vuex-router-sync": "^5.0.0",
    "vue-server-renderer": "^2.5.8"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.3",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.49",
    "@types/source-map": "^0.5.2",
    "@types/uglify-js": "^2.6.29",
    "@types/vue": "^2.0.0",
    "autodll-webpack-plugin": "^0.3.3",
    "autoprefixer": "^7.1.6",
    "avoriaz": "^6.0.1",
    "awesome-typescript-loader": "^3.3.0",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "brotli-webpack-plugin": "^0.5.0",
    "bundlesize": "^0.15.3",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.1",
    "cssnano": "^3.10.0",
    "csx": "^8.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-critical-webpack-plugin": "^1.0.1",
    "html-elements-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jest": "^21.2.1",
    "jest-vue": "^0.8.2",
    "npm-run-all": "^4.1.2",
    "npm-scripts-help": "^0.7.0",
    "offline-plugin": "^4.8.4",
    "optimize-js-plugin": "0.0.4",
    "postcss-loader": "^2.0.8",
    "preload-webpack-plugin": "^2.0.0",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.2.1",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslib": "^1.5.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-loader": "^3.5.1",
    "typescript": "2.6.2",
    "typestyle": "^1.5.1",
    "vue-loader": "^13.4.0",
    "vue-property-decorator": "^6.0.0",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "3.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.es2015.json",
        "useBabelrc": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katallaxie/vue-preboot.git"
  },
  "bugs": {
    "url": "https://github.com/katallaxie/vue-preboot/issues"
  },
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3"
  }
}
